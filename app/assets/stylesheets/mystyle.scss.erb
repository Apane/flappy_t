@font-face {
  font-family: 'ProximaNovaRegular';
  src: font-url("<%= asset_path('proximanova-regular-webfont.eot') %>");
  src: font-url("<%= asset_path('proximanova-regular-webfont.eot') %>") format('embedded-opentype'),
  font-url("<%= asset_path('proximanova-regular-webfont.woff') %>") format('woff'),
  font-url("<%= asset_path('proximanova-regular-webfont.ttf') %>") format('truetype'),
  font-url("<%= asset_path('proximanova-regular-webfont.svg#ProximaNovaRegular') %>") format('svg');
}

@font-face {
  font-family: 'ProximaNovaBold';
  src: font-url("<%= asset_path('proximanova-bold-webfont.eot') %>");
  src: font-url("<%= asset_path('proximanova-bold-webfont.eot') %>") format('embedded-opentype'),
  font-url("<%= asset_path('proximanova-bold-webfont.woff') %>") format('woff'),
  font-url("<%= asset_path('proximanova-bold-webfont.ttf') %>") format('truetype'),
  font-url("<%= asset_path('proximanova-bold-webfont.svg#ProximaNovaBold') %>") format('svg');
}

@font-face {
  font-family: 'boxyfont';
  src: font-url("<%= asset_path('04b_19__-webfont.eot') %>");
  src: font-url("<%= asset_path('04b_19__-webfont.eot?#iefix') %>") format('embedded-opentype'),
  font-url("<%= asset_path('04b_19__-webfont.svg#04B_19__') %>") format('svg'),
  font-url("<%= asset_path('04b_19__-webfont.woff') %>") format('woff'),
  font-url("<%= asset_path('04b_19__-webfont.tff') %>") format('truetype');
}

.first {font-family: 'noteworthy'; font-weight: bold; margin: 0 auto; width: 500px; text-align:center;}
.first-p {font-family: 'noteworthy'; font-weight: regular; margin: 0 auto; width: 500px; text-align:center;padding-top:20px;font-size: 20px;color:#808080;}

#space  {padding: 20px 0 20px 0;}
#space2 {margin: 20px 0 20px 0;}

.font1{
    font-family: 'boxyfont';
    text-shadow: -1px -1px white, -1px 1px white, 2px -1px white, -1px -1px white;
    font-size: 40px;
}

.fontone{
    padding-left: 20px;
    font-family: 'boxyfont';
    text-shadow: -1px -1px white, -1px 1px white, 2px -1px white, -1px -1px white;
    font-size: 40px;
}

.fonttwo{
    font-family: 'boxyfont';
    color: #F2BB0C;
    text-shadow: 2px -1px black, -1px 2px black, 2px 2px black, 2px -1px black;
}

.font2{
    font-family: 'noteworthy';
    padding: 4px 5px 4px 5px;
    color: white;
    font-weight: bold;
    text-shadow: 1px 0 black, 0 2px black, 2px 0 black, 0 -1px black;
}
.font3{
    font-family: 'boxyfont';
    color: white;
    font-size: 40px;
    font-weight: bold;
    text-shadow: -1px -2px black, -2px 2px black, -2px -1px black, 2px -1px black;
}

.font3 img{
    margin-top: -45px;
}

.font2 img{
    margin-top: -45px;
}

.flappy-summary {
  font-family: 'noteworthy';
  font-size: 25px;
  text-align: center;
  padding-bottom: 20px;
}

.pledge {
  margin: 0 20px 10px 20px;
  font-size: 12px;
  color: #999999;
}

#reserve_container p {
  color: white !important;
}
